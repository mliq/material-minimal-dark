  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#5A5A5A"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#71bef9"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F77669"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#F77669"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#F77669"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#91B859"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": ""
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#c081b6"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#EC5F67",
        "fontStyle": "",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#D3423E",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#F77669"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#F77669"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#7DC9E3"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#ddbb88"
      }
    },
    {
      "name": "Tag attribute id",
      "scope": [
        "meta.toc-list.id",
        "meta.attribute-with-value.id punctuation.definition.string.begin.html",
        "meta.attribute-with-value.id punctuation.definition.string.end.html"
      ],
      "settings": {
        "foreground": "#7DC9E3"
      }
    },
    {
      "name": "Property name",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#80CBC4"
      }
    },
    {
      "name": "Unit keyword",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#FFEB95"
      }
    },
    {
      "name": "JS \"this\"",
      "scope": "variable.language.this.js",
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Support type",
      "scope": "support.type",
      "settings": {
        "foreground": "#FFEB95"
      }
    },
    {
      "name": "CSS Custom Property",
      "scope": "variable.css",
      "settings": {
        "foreground": "#B7E1DF"
      }
    },
    {
      "name": "Meta selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Support type property name variable",
      "scope": "support.type.property-name.variable",
      "settings": {
        "foreground": "#CEF2EF"
      }
    },
    {
      "name": "String quoted",
      "scope": "string.quoted",
      "settings": {
        "foreground": "#B0CD89"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#94b9d8"
      }
    },
    {
      "scope": [
        "keyword - keyword.operator",
        "keyword.control",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#c081b6"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e343b0"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#e343b0"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#e343b0"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#e343b0"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#e343b0"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#71bef9"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#71bef9"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#6c98f7"
      }
    },
    {
      "scope": "entity.name.variable",
      "settings": {
        "foreground": "#6c98f7"
      }
    }
  ]
